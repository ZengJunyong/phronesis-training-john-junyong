{
  "type": "module",
  "name": "rebilly-js-sdk",
  "version": "56.61.1",
  "description": "Official Rebilly API JS library for the browser and Node",
  "types": "./dist/rebilly-js-sdk.d.ts",
  "main": "./dist/rebilly-js-sdk.umd.js",
  "module": "./dist/rebilly-js-sdk.es.js",
  "exports": {
    ".": {
      "import": "./dist/rebilly-js-sdk.es.js",
      "require": "./dist/rebilly-js-sdk.umd.js"
    },
    "./types": "./dist/rebilly-js-sdk.d.ts"
  },
  "scripts": {
    "build": "vite build && yarn ts:bundle-types && yarn test:check-build-types",
    "build:dev": "vite build --mode dev",
    "clean": "rimraf dist && rimraf coverage",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:check-build-types": "tsc -p ./test/check-build-types.tsconfig.json",
    "ts:generate-types": "node ./scripts/type-generation/generate-ts-types.js",
    "ts:compile-and-merge": "tsc -p tsc-emit.json --typeRoots typings && node ./scripts/merge-types.js",
    "ts:bundle-types": "yarn ts:generate-types && yarn ts:compile-and-merge"
  },
  "repository": "https://github.com/Rebilly/rebilly",
  "keywords": [
    "Rebilly",
    "js",
    "sdk",
    "library",
    "node"
  ],
  "files": [
    "dist",
    "CHANGELOG.md",
    "src",
    "scripts",
    "test",
    "types-generation",
    "tsc-emit.json",
    "vite.config.js"
  ],
  "author": "Rebilly",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Rebilly/rebilly-js-sdk/issues"
  },
  "homepage": "https://github.com/Rebilly/rebilly-js-sdk#readme",
  "devDependencies": {
    "@rebilly/eslint-config": "*",
    "@types/jest": "^29.5.1",
    "axios-mock-adapter": "^2.0.0",
    "c8": "^10.1.2",
    "clone-deep": "^4.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "faker": "^4.1.0",
    "openapi-typescript": "3.4.1",
    "typescript": "^5.4.5",
    "vite": "^5.4.9",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "nanoid": "^5.0.7"
  },
  "resolutions": {
    "glob-parent": "^5.1.2",
    "serialize-javascript": "^3.1.0"
  }
}
